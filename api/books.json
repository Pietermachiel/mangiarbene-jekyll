---
layout: null
---

[ 
    {% for book in site.books %}
        {
            "index"    : "{{ book.index }}",
            "title"    : "{{ book.title }}",
            "id"       : "{{ book.title | slugify }}",
            "author"   : "{{ book.author }}",
            "publisher": "{{ book.publisher }}",
            "year"     : "{{ book.year }}",
            "kitchen"  : "{{ book.kitchen }}",
            "link"     : "{{ book.link }}",
            "book_url" : "{{ book.book_url }}",
            "text"  : "{{ book.text }}",
            "content"  : {{ book.content | jsonify }}
        }{% unless forloop.last %},{% endunless %}{% endfor %}
        
]